# Redirect para Links Rastreáveis - Belitx
# Redireciona /r/CODIGO para index.php

RewriteEngine On
RewriteBase /r/

# Se não for um arquivo ou diretório existente, redirecionar para index.php
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php [L,QSA]
